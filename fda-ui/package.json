{
  "name": "fda-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 3001",
    "docker": "nuxt > /dev/null",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "ver": "nuxt --version",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "ava --verbose",
    "test:watch": "ava --watch"
  },
  "ava": {
    "require": [
      "./test/setup.js",
      "@babel/polyfill"
    ],
    "timeout": "20s",
    "files": [
      "test/unit/**/*",
      "test/e2e/**/*"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@mdi/js": "^5.8.55",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/proxy": "^2.1.0",
    "axios-mock-adapter": "^1.19.0",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "is-docker": "^2.2.1",
    "knex": "^0.95.6",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.12.2",
    "nuxt-i18n": "^6.15.4",
    "vue-toast-notification": "^0.5.4",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "@babel/core": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-plugin-nuxt": "^1.0.0"
  }
}
