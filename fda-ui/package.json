{
  "name": "fda-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 3001",
    "docker": "nuxt > /dev/null",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "ver": "nuxt --version",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test:unit": "cross-env TEST=unit ava --config ava.config.cjs ./test/unit/**/* ./test/specs/**/*",
    "test:watch": "cross-env TEST=unit ava --config ava.config.cjs --watch",
    "test:e2e": "cross-env TEST=e2e ava --config ava.config.cjs ./test/e2e/**/*",
    "test": "yarn test:unit & yarn test:e2e"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@mdi/js": "^5.8.55",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/proxy": "^2.1.0",
    "axios-mock-adapter": "^1.19.0",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "is-docker": "^2.2.1",
    "knex": "^0.95.6",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.12.2",
    "nuxt-i18n": "^6.15.4",
    "vue-toast-notification": "^0.5.4",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.2.0",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.10.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "nyc": "^15.1.0",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^3.0.0"
  }
