    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.15.4",
    "sql-formatter": "^4.0.2",
    "vue": "^2.6.12",
    "vue-toast-notification": "^0.5.4",
    "vuex": "^3.6.2",
