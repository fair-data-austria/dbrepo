    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "eslint": "^7.27.0",
    "express": "^4.17.1",
    "highlight.js": "^10.7.2",
    "is-docker": "^2.2.1",
    "knex": "^0.95.6",
    "multer": "^1.4.2",
